# focus windows
alt - h : yabai -m window --focus west
alt - l : yabai -m window --focus east
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north

# move windows
shift+alt - h : yabai -m window --swap west
shift+alt - l : yabai -m window --swap east
shift+alt - j : yabai -m window --swap south
shift+alt - k : yabai -m window --swap north

# resize windows
ctrl+alt - h : yabai -m window --resize left:-30:0
ctrl+alt - l : yabai -m window --resize right:30:0
ctrl+alt - j : yabai -m window --resize bottom:0:30
ctrl+alt - k : yabai -m window --resize top:0:-30


alt - f : yabai -m window --toggle zoom-fullscreen
alt - g : yabai -m window --toggle zoom-parent


# Focus display 1 with alt + 1
alt - 1 : yabai -m display --focus 1

# Focus display 2 with alt + 2
alt - 2 : yabai -m display --focus 2

# Move to display 1
alt + shift - 1 : yabai -m window --display 1; yabai -m display --focus 1

# Move to display 2
alt + shift - 2 : yabai -m window --display 2; yabai -m display --focus 2
